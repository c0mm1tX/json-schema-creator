{
  "name": "json-schema-creator",
  "version": "1.0.0",
  "description": "TypeScript library for dynamically creating JSON Schema compatible with AJV",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint && npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c0mm1tX/json-schema-creator.git"
  },
  "keywords": [
    "json",
    "schema",
    "creator",
    "ajv",
    "validation",
    "typescript",
    "json-schema",
    "builder",
    "fluent"
  ],
  "author": "Mariusz Lejkowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c0mm1tX/json-schema-creator/issues"
  },
  "homepage": "https://github.com/c0mm1tX/json-schema-creator#readme",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
